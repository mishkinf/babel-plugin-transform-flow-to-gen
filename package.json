{
  "name": "babel-plugin-transform-flow-to-gen",
  "version": "0.0.14-0",
  "description": "Turn flow definitions into generator functions",
  "main": "lib/index.js",
  "author": "Gabe Scholz",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "DEBUG=1 npm run test -- --watchAll --noCache",
    "lint": "eslint --fix --ext .js src/",
    "precommit": "npm run test && npm run lint",
    "prepublish": "rm -rf lib && babel src --out-dir lib --ignore __test__,test.js",
    "postpublish": "rm -rf lib",
    "release": "np"
  },
  "dependencies": {
    "babel-plugin-syntax-flow": "^6.18.0",
    "testcheck": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-generator": "^6.21.0",
    "babel-plugin-transform-flow-comments": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-traverse": "^6.21.0",
    "babylon": "^6.14.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.2",
    "jest": "^18.1.0",
    "np": "^2.12.0",
    "prettier": "^0.22.0",
    "prettier-eslint": "^4.3.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "syntax-flow",
      "transform-flow-comments"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    },
    "prettierOptions": {
      "singleQuote": true,
      "brackSpacing": false,
      "trailingComma": "all",
      "parser": "flow",
      "printWidth": 100,
      "write": "**/*.js"
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "backtick"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "no-use-before-define": 0,
      "global-require": 0
    }
  },
  "jest": {
    "moduleNameMapper": {
      "babel-plugin-transform-flow-to-gen/lib/typeHelpers": "<rootDir>/src/typeHelpers.js",
      "babel-plugin-transform-flow-to-gen/lib/sampleOne": "<rootDir>/src/sampleOne.js",
      "babel-plugin-transform-flow-to-gen/Gen": "<rootDir>/Gen.js"
    }
  }
}
