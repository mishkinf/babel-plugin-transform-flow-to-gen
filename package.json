{
  "name": "babel-plugin-transform-flow-to-gen",
  "version": "0.0.3",
  "description": "Turn flow definitions into generator functions",
  "main": "lib/index.js",
  "author": "Gabe Scholz",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix --ext .js src/",
    "prepublish": "rm -rf lib && babel src --out-dir lib --ignore __test__,test.js",
    "release": "np"
  },
  "dependencies": {
    "babel-plugin-syntax-flow": "^6.18.0",
    "testcheck": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-generator": "^6.21.0",
    "babel-plugin-transform-flow-comments": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-traverse": "^6.21.0",
    "babylon": "^6.14.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.0.0",
    "np": "^2.12.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "syntax-flow",
      "transform-flow-comments"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base"
    ],
    "plugins": [],
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "backtick"
      ],
      "no-underscore-dangle": 0,
      "import/no-extraneous-dependencies": 0,
      "no-use-before-define": 0,
      "global-require": 0,
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ]
    }
  },
  "jest": {
    "moduleNameMapper": {
      "babel-plugin-transform-flow-to-gen/types": "<rootDir>/src/typeHelpers.js",
      "babel-plugin-transform-flow-to-gen/sample": "<rootDir>/src/sample.js"
    }
  }
}
